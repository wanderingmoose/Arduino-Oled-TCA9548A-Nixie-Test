void clearAllDisplays() {
  for (uint8_t i = 0; i < 5; i++) {
    clearDigit(i);
  }
}

void clearDigit(uint8_t displayNum) {
  TCA9548A(displayNum);
  display.clearDisplay();
  display.drawBitmap(0, 0, blank, 128, 64, SSD1306_WHITE);
  //display.drawBitmap(0, 0, blank, 8, 8, SSD1306_WHITE);
  display.display();
}

void displayTime(int hours, int minutes, bool showColon) {
  int hourTens = hours / 10;
  int hourOnes = hours % 10;
  int minuteTens = minutes / 10;
  int minuteOnes = minutes % 10;

  // Display the digits in reversed order
  if (hourTens == 0) {
    clearDigit(0); // Clear the leading zero if hourTens is 0
  } else {
    displayDigit(0, hourTens);
  }
  displayDigit(1, hourOnes);
  if (showColon) {
    displayColon(2);
  } else {
    clearDigit(2);
  }
  displayDigit(3, minuteTens);
  displayDigit(4, minuteOnes);
}

void displayDigit(uint8_t displayNum, int digit) {
  TCA9548A(displayNum);
  display.clearDisplay();
  display.drawBitmap(0, 0, numbers[digit], 128, 64, SSD1306_WHITE);
  //display.drawBitmap(0, 0, numbers[digit], 8, 8, SSD1306_WHITE);
  display.display();
}

void displayColon(uint8_t displayNum) {
  TCA9548A(displayNum);
  display.clearDisplay();
  display.drawBitmap(0, 0, colon, 128, 64, SSD1306_WHITE);
  //display.drawBitmap(0, 0, colon, 8, 8, SSD1306_WHITE);
  display.display();
}
