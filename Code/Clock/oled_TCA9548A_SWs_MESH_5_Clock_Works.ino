void setup() {
  Wire.begin();
  //Serial.begin(9600);
  pinMode(ledPin, OUTPUT); // Set the LED pin as an output

  // Initialize all displays
  for (uint8_t i = 0; i < 5; i++) {
    TCA9548A(i);
    if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
      for (;;);
    }
    display.clearDisplay();
    display.display();
    delay(500);
  }

  // Initialize the RTC
  TCA9548A(6); // Select I2C bus 6 for the RTC
  if (!rtc.begin()) {
    Serial.println("Couldn't find RTC");
    while (1);
  }

  if (!rtc.isrunning()) {
    //Serial.println("RTC is NOT running!");
    // Set the RTC to the date & time this sketch was compiled
    rtc.adjust(DateTime(F(__DATE__), F(__TIME__)));
  }
}

void loop() {
  static bool showColon = false;
  TCA9548A(6); // Ensure we're communicating with the RTC on bus 6
  DateTime now = rtc.now();
  
  // Get the current time
  int hours = now.hour();
  int minutes = now.minute();
  int seconds = now.second();

  // Convert to 12-hour format and determine AM/PM
  bool isPM = false;
  if (hours >= 12) {
    isPM = true;
    if (hours > 12) {
      hours -= 12;
    }
  } else if (hours == 0) {
    hours = 12;
  }

  // Display the time
  displayTime(hours, minutes, showColon);

  // Control the LED for AM/PM indication
  digitalWrite(ledPin, isPM ? HIGH : LOW);

  // Toggle colon every 1 seconds
  showColon = !showColon;

  delay(1000); // Wait for 1 seconds
}
