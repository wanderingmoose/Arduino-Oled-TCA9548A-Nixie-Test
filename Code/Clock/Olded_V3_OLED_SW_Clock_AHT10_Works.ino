// Function prototypes
void TCA9548A(uint8_t bus);
void clearAllDisplays();
void clearDigit(uint8_t displayNum);
void displayTime(int hours, int minutes, bool showColon);
void displayDigit(uint8_t displayNum, int digit);
void displayColon(uint8_t displayNum);
void showTemperatureHumidity();
void displayCenterText();

void setup() {
  Wire.begin();
  pinMode(ledPin, OUTPUT); // Set the LED pin as an output

  // Initialize all displays
  for (uint8_t i = 0; i < 5; i++) {
    TCA9548A(i);
    if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
      for (;;);
    }
    display.clearDisplay();
    display.display();
    delay(500);
  }

  // Initialize the RTC
  TCA9548A(6); // Select I2C bus 6 for the RTC
  if (!rtc.begin()) {
    while (1);
  }
  rtc.adjust(DateTime(F(__DATE__), F(__TIME__)));
  if (!rtc.isrunning()) {
    rtc.adjust(DateTime(F(__DATE__), F(__TIME__)));
  }

  // Initialize the AHT10 sensor
  TCA9548A(7); // Select I2C bus 7 for the AHT10
  if (!aht.begin()) {
    while (1);
  }
}

void loop() {
  unsigned long currentMillis = millis();

  // Handle the time display and temperature/humidity display based on seconds
  TCA9548A(6); // Ensure we're communicating with the RTC on bus 6
  DateTime now = rtc.now();
  
  // Get the current time
  int hours = now.hour();
  int minutes = now.minute();
  int seconds = now.second();

  // Convert to 12-hour format and determine AM/PM
  bool isPM = false;
  if (hours >= 12) {
    isPM = true;
    if (hours > 12) {
      hours -= 12;
    }
  } else if (hours == 0) {
    hours = 12;
  }

  // Check if we should start displaying temperature and humidity
  if (seconds == 30 && !displayingTempHumidity) {
    displayingTempHumidity = true;
    tempHumidityDisplayStart = currentMillis;
    showTemperatureHumidity();
  }

  // Check if we should stop displaying temperature and humidity
  if (displayingTempHumidity) {
    if (currentMillis - tempHumidityDisplayStart >= 5000) {
      displayingTempHumidity = false;
      clearAllDisplays();
    }
  }

  // Update the time display every 500ms
  if (!displayingTempHumidity && (currentMillis - lastUpdate) >= 1000) {
  //  if (!displayingTempHumidity && (currentMillis - lastUpdate) >= 500) {
    lastUpdate = currentMillis;

    // Display the time
    displayTime(hours, minutes, showColon);

    // Control the LED for AM/PM indication
    digitalWrite(ledPin, isPM ? HIGH : LOW);

    // Toggle colon every 0.5 seconds
    showColon = !showColon;
  }
}

void clearAllDisplays() {
  for (uint8_t i = 0; i < 5; i++) {
    clearDigit(i);
  }
}

void clearDigit(uint8_t displayNum) {
  TCA9548A(displayNum);
  display.clearDisplay();
  display.display();
}

void displayTime(int hours, int minutes, bool showColon) {
  int hourTens = hours / 10;
  int hourOnes = hours % 10;
  int minuteTens = minutes / 10;
  int minuteOnes = minutes % 10;

  // Display the digits in reversed order
  if (hourTens == 0) {
    clearDigit(0); // Clear the leading zero if hourTens is 0
  } else {
    displayDigit(0, hourTens);
  }
  displayDigit(1, hourOnes);
  if (showColon) {
    displayColon(2);
  } else {
    clearDigit(2);
  }
  displayDigit(3, minuteTens);
  displayDigit(4, minuteOnes);
}

void displayDigit(uint8_t displayNum, int digit) {
  TCA9548A(displayNum);
  display.clearDisplay();
  display.drawBitmap(0, 0, numbers[digit], 128, 64, SSD1306_WHITE);
  //display.drawBitmap(0, 0, numbers[digit], 8, 8, SSD1306_WHITE);
  display.display();
}

void displayColon(uint8_t displayNum) {
  TCA9548A(displayNum);
  display.clearDisplay();
  display.drawBitmap(0, 0, colon, 128, 64, SSD1306_WHITE);
  //display.drawBitmap(0, 0, colon, 8, 8, SSD1306_WHITE);
  display.display();
}

void showTemperatureHumidity() {
  sensors_event_t humidity, temp;
  TCA9548A(7); // Ensure we're communicating with the AHT10 on bus 7
  aht.getEvent(&humidity, &temp);

  int temperature = round(temp.temperature);
  int humidityVal = round(humidity.relative_humidity);

  // Show temperature on first 2 displays
  int tempTens = temperature / 10;
  int tempOnes = temperature % 10;

  if (tempTens == 0) {
    clearDigit(0); // Clear the leading zero if tempTens is 0
  } else {
    displayDigit(0, tempTens);
  }
  displayDigit(1, tempOnes);

  // Show humidity on last 2 displays
  int humTens = humidityVal / 10;
  int humOnes = humidityVal % 10;

  displayDigit(3, humTens);
  displayDigit(4, humOnes);

  // Display "Temp:" and "RH%" on the center display (display 2)
  displayCenterText();
}

void displayCenterText() {
  TCA9548A(2); // Select the center display (display 2)
  display.clearDisplay();
  display.setRotation(1); // Set rotation to display text sideways
  display.setTextSize(3); // Adjust size as needed
  display.setTextColor(SSD1306_WHITE);
  display.setCursor(24, 16);
  display.print(F("C"));
  display.setCursor(0, 36);
  display.print(F("<--"));
  display.setCursor(23, 76);
  display.print(F("%"));
  display.setCursor(8, 96);
  display.print(F("-->"));
  display.display();
  display.setRotation(0);
}
