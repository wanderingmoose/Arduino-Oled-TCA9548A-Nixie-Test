void setup() {
  Wire.begin();
  Serial.begin(9600);

  // Initialize all displays
  for (uint8_t i = 0; i < 5; i++) {
    TCA9548A(i);
    if (!display.begin(SSD1306_SWITCHCAPVCC, 0x3C)) {
      for (;;);
    }
    display.clearDisplay();
    display.display();
    delay(500);
  }

  // Initialize the AHT10 sensor
  TCA9548A(7); // Select I2C bus 7 for the AHT10
  if (!aht.begin()) {
    Serial.println("Could not find AHT10 sensor!");
    while (1);
  }
  Serial.println("AHT10 sensor initialized.");
}

void loop() {
  sensors_event_t humidity, temp;

  // Display temperature for 5 seconds
  TCA9548A(7); // Ensure we're communicating with the AHT10 on bus 7
  aht.getEvent(&humidity, &temp);
  float temperature = temp.temperature;
  displayFloat(temperature);
  delay(5000);

  // Clear display
  clearAllDisplays();

  // Display humidity for 5 seconds
  TCA9548A(7); // Ensure we're communicating with the AHT10 on bus 7
  aht.getEvent(&humidity, &temp);
  float hum = humidity.relative_humidity;
  displayFloat(hum);
  delay(5000);

  // Clear display
  clearAllDisplays();
}

void clearAllDisplays() {
  for (uint8_t i = 0; i < 5; i++) {
    clearDigit(i);
  }
}

void clearDigit(uint8_t displayNum) {
  TCA9548A(displayNum);
  display.clearDisplay();
  //display.drawBitmap(0, 0, blank, 8, 8, SSD1306_WHITE);
   display.drawBitmap(0, 0, blank, 128, 64, SSD1306_WHITE);
  display.display();
}

void displayFloat(float value) {
  bool isNegative = value < 0;
  if (isNegative) value = -value;

  int intPart = (int)value;
  int decimalPart = (int)((value - intPart) * 10); // Get one digit after the decimal point

  int thousands = intPart / 1000;
  int hundreds = (intPart / 100) % 10;
  int tens = (intPart / 10) % 10;
  int ones = intPart % 10;

  int displayIndex = 4; // Start from the rightmost display

  // Display decimal part
  displayDigit(displayIndex, decimalPart);
  displayIndex--;

  // Display decimal point
  displayDecimalPoint(displayIndex);
  displayIndex--;

  // Display ones digit
  displayDigit(displayIndex, ones);
  displayIndex--;

  // Display tens digit, or blank if unnecessary
  if (thousands == 0 && hundreds == 0 && tens == 0 && !isNegative) {
    clearDigit(displayIndex);
  } else {
    displayDigit(displayIndex, tens);
  }
  displayIndex--;

  // Display hundreds digit, or blank if unnecessary
  if (thousands == 0 && hundreds == 0 && !isNegative) {
    clearDigit(displayIndex);
  } else {
    displayDigit(displayIndex, hundreds);
  }
  displayIndex--;

  // Display thousands digit, or blank if unnecessary
  if (thousands == 0 && !isNegative) {
    clearDigit(displayIndex);
  } else {
    displayDigit(displayIndex, thousands);
  }
  displayIndex--;

  // Display minus sign if needed
  if (isNegative) {
    displayDigit(displayIndex, -1);
  } else {
    clearDigit(displayIndex);
  }
}

void displayDigit(uint8_t displayNum, int digit) {
  TCA9548A(displayNum);
  display.clearDisplay();
  if (digit == -1) {
    display.drawBitmap(0, 0, minus, 128, 64, SSD1306_WHITE);
    //display.drawBitmap(0, 0, minus, 8, 8, SSD1306_WHITE);
  } else {
    display.drawBitmap(0, 0, numbers[digit], 128, 64, SSD1306_WHITE);
    //display.drawBitmap(0, 0, numbers[digit], 8, 8, SSD1306_WHITE);
  }
  display.display();
}

void displayDecimalPoint(uint8_t displayNum) {
  TCA9548A(displayNum);
  display.clearDisplay();
  display.drawBitmap(0, 0, dot, 128, 64, SSD1306_WHITE);
  //display.drawBitmap(0, 0, dot, 8, 8, SSD1306_WHITE);
  display.display();
}
